<% provide(:title, @console_name.eng_name) %>

<% if signed_in? %>
<%= link_to "Can't find a console?", new_console_path, class: "pull-right" %>
<% end %>

<h1 style="text-align: center"><%= params[:id] %> Consoles</h1>

<div class="browse_list offset3" id="game_info">
<ul>
  <% @console_games.each do |game| %>

<li><%= link_to game.eng_name, current_console(game) %> </li>

<% end %>

</ul>
</div>
