<%= render layout: 'layouts/inventorySideBar' do %>
    <% if (!@ownership.empty?) %>
        <li><%= link_to "All Consoles",  inventory_consoles_path(current_user) %></li>
        <% @consoleConsoles.each do |game| %>
            <li><%= link_to game, inventory_consoles_path(current_user) + '/' + game %> </li>
        <% end %>
    <% end %>
<% end %>
